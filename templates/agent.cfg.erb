<% if @no_proxy == false -%>
proxy = http://proxy_url:port
<% end -%>
server = <%= @server_url %>
<% if @user != nil -%>
user = <%= @user %>
<% end -%>
<% if @password != nil -%>
password = <%= @password %>
<% end -%>
<% if !@embedded_http -%>
no-httpd=1
<% end -%>
<% if @no_ssl_check -%>
no-ssl-check = 1
<% end -%>
